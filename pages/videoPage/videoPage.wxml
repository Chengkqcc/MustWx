<!--pages/videoPage/videoPage.wxml-->
<view class="video">
    <view class="nav">
        <view class="icon">
            <van-icon name="arrow-left" size="20px" color="#aaa" bindtap="back"/>
        </view>
        <view class="title">{{text}}</view>
    </view>
    <view class="content">
        <!-- 账户注册 -->
        <view class="register" wx:if="{{register}}">
            <view class="videoWrap">
                 <video src="{{videoSrc}}"></video>
            </view>
        </view>
    </view>
</view>
