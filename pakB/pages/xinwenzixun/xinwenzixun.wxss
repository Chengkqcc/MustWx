page {
  background-color: #F1F1F1;
}
.xinwenzixun {
  height: 100vh;
}
.xinwenzixun .img {
  height: 150px;
}
.xinwenzixun .img image {
  width: 100%;
  height: 100%;
}
.xinwenzixun .tabs .van-tabs__nav {
  border-bottom: 1px solid #F1F1F1;
}
.xinwenzixun .tabs .item {
  padding: 15px;
  display: flex;
  justify-content: space-between;
  background-color: #fff;
  border-bottom: 1px solid #F1F1F1;
}
.xinwenzixun .tabs .item .title {
  margin-right: 20px;
}
.xinwenzixun .tabs .item .title .text {
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 14px;
  color: rgba(59, 65, 68, 0.91);
}
.xinwenzixun .tabs .item .title .label {
  margin-top: 3px;
  color: #999;
  font-size: 12px;
}
.xinwenzixun .tabs .item .van-icon {
  color: #bbbbbb;
  font-size: 16px;
}
.xinwenzixun .tabs .empty {
  height: calc(100vh - 120px);
}
.xinwenzixun .tabs .empty .van-empty {
  background-color: #F1F1F1;
}
